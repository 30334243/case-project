{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 23,
        "patch": 0
    },
    "include": [
        "../msys.json"
    ],
    "configurePresets": [
        {
            "name": "msys-application",
            "hidden": true,
            "inherits": [
                "msys"
            ],
            "cacheVariables": {
                "IS_APPLICATION": "ON"
            },
            "condition": {
                "type": "equals",
                "lhs": "${hostSystemName}",
                "rhs": "Windows"
            }
        },
        {
            "name": "msys-application-debug",
            "inherits": [
                "msys-application",
                "msys-debug"
            ],
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "msys-application-release",
            "inherits": [
                "msys-application",
                "msys-release"
            ],
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        }
    ]
}
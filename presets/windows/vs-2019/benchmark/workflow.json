{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 23,
        "patch": 0
    },
    "include": [
        "config.json",
        "build.json",
        "test.json",
        "package.json"
    ],
    "workflowPresets": [
        {
            "name": "vs-2019-benchmark-ci",
            "steps": [
                {
                    "type": "configure",
                    "name": "vs-2019-benchmark"
                },
                {
                    "type": "build",
                    "name": "vs-2019-benchmark-debug"
                },
                {
                    "type": "build",
                    "name": "vs-2019-benchmark-release"
                },
                {
                    "type": "package",
                    "name": "vs-2019-benchmark-package"
                },
                {
                    "type": "test",
                    "name": "vs-2019-benchmark-debug-test"
                },
                {
                    "type": "test",
                    "name": "vs-2019-benchmark-release-test"
                },
                {
                    "type": "package",
                    "name": "vs-2019-benchmark-package"
                }
            ]
        }
    ]
}